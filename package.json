    "drizzle-kit": "^0.31.1",
    "drizzle-prisma-generator": "^0.1.7",
    "eslint": "^9.26.0",
    "eslint-config-next": "15.3.1",
    "patch-package": "^8.0.0",
    "prisma": "^6.7.0",
    "tailwindcss": "^4.1.5",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "wrangler": "^4.14.1"
