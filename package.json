    "@opennextjs/cloudflare": "1.0.0-beta.4",
    "@prisma/client": "6.7.0",
    "@tryghost/content-api": "^1.11.22",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "next": "15.3.1",
    "next-auth": "5.0.0-beta.27",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250503.0",
    "@eslint/eslintrc": "^3.3.1",
